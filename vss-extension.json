{
    "manifestVersion": 1,
    "id": "vsts-extension-sprint-replay",
    "BaseUri": "http://localhost:8520/",
    "version": "0.1.11",
    "name": "Sprint Replay",
    "scopes": [
        "vso.work",
        "vso.work_write"
    ],
    "description": "Replay board",
    "publisher": "blueprint",
    "public": false,
    "icons": {
        "default": "img/logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "files": [
        {
            "path": "img",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "index.html",
            "addressable": true
        }
    ],
    "categories": [
        "Plan and track"
    ],
    "contributions": [
        {
            "id": "vsts-extension-sprint-replay",
            "type": "ms.vss-web.hub",
            "description": "",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Sprint Replay",
                "uri": "index.html"
            }
        }
    ]
}